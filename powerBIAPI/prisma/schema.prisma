generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

// ========================== Models for A to D

// etl_adverse_events
model AdverseEvents{
  uuid String?
  mfl_code Int?
  form String?
  provider Int?
  patient_id Int @id
  visit_id Int?
  visit_date DateTime? @db.Date
  location_id Int?
  encounter_id Int?
  obs_id Int?
  cause Int?
  adverse_event Int?
  severity Int?
  start_date DateTime ?   @db.Date
  action_taken Int?
  voided Int?  
  date_created DateTime?   @db.Date
  date_last_modified DateTime?   @db.Date

  @@map("etl_adverse_events")
}

